{
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/css" // Path where compiled CSS files will be saved
    }
  ],
  "liveSassCompile.settings.includeItems": [
    "/sass/**/*.scss" // Path where the SCSS files are located
  ],
  "liveSassCompile.settings.excludeList": ["**/node_modules/**", ".vscode/**"]
}
